<%= form_with model: @prototype, local: true do |f| %>
  <div class="field">
    <%= f.label :name, 'プロトタイプの名称' %><br>
    <%= f.text_field :name, id: 'prototype_name' %>
  </div>

  <div class="field">
    <%= f.label :catch_copy, 'キャッチコピー' %><br>
    <%= f.text_field :catch_copy, id: 'prototype_catch_copy' %>
  </div>

  <div class="field">
    <%= f.label :concept, 'コンセプト' %><br>
    <%= f.text_area :concept, rows: 5, id: 'prototype_concept' %>
  </div>

  <div class="field">
    <%= f.label :image, '画像' %><br>
    <%= f.file_field :image, accept: 'image/*' %>
  </div>

  <%= f.submit '投稿する' %>
<% end %>
